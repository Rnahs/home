import requests
from bs4 import BeautifulSoup as bs

def main():
    list_title = []

    for i in range(23,29):
        url = "https://www.kyeonggi.com/list/"+str(i)
        html = requests.get(url)
        #print(html.text)
        soup = bs(html.text,'html.parser')
        #print(soup)

    #soup.select가 있음. 단서를 많이 포함할 수있음.

        for news_title in soup.find_all('h3'):
            #print(news_title.get_text())
            list_title.append(news_title.get_text())
    #for문을 사용하는 두가지 방식
    # for i in range(숫자 숫자 숫자):
    # i를 통해 arr[i]= 4.. 이런식으로 사용가능
    # for j in list:
    # j = 4 라고 했을때 리스트값이 변할까?
    # j가 list[0]번째부터 끝까지. 한번씩 '참조'
    #

    #print(list_title)

    for i in list_title:
        print(i)
        print()
if name=='main':
    main()
